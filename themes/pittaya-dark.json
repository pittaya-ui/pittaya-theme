  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#707070",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": "meta.block variable.other",
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Language Variables (this, self)",
      "scope": "variable.language",
      "settings": {
        "foreground": "#ff637e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#ffb3cc"
      }
    },
    {
      "name": "Color Constants",
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff3333"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.return",
        "keyword.control.loop",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ff637e"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#c5c5c5"
      }
    },
    {
      "name": "Special Operators",
      "scope": [
        "keyword.other.substitution",
        "meta.brace.round",
        "meta.brace.square",
        "punctuation.definition.parameters",
        "punctuation.definition.array"
      ],
      "settings": {
        "foreground": "#c5c5c5"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#8fd460"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#ff637e"
      }
    },
    {
      "name": "Types and Interfaces",
      "scope": [
        "support.type",
        "entity.name.type",
        "entity.name.type.interface",
        "entity.name.type.alias"
      ],
      "settings": {
        "foreground": "#b8e986"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#ffcc66"
      }
    },
    {
      "name": "String Links",
