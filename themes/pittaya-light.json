	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#909090",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#2a2a2a"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": "meta.block variable.other",
			"settings": {
				"foreground": "#2a2a2a"
			}
		},
		{
			"name": "Language Variables (this, self)",
			"scope": "variable.language",
			"settings": {
				"foreground": "#d91656",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#c2255c"
			}
		},
